  "content": "<!-- wp:themepark-block/themepark-area {\"maxw\":true,\"title\":\"\",\"title2\":\"\",\"title1mode\":\"modle_title3\",\"title1color\":\"#000000\",\"title2color\":\"#ef8800\",\"titlecolor\":\"#5d5d5d\",\"title1size\":24,\"title2size\":18,\"titleboder\":2,\"titletext\":\"\",\"seo2\":\"H3\",\"blockbtn\":\"\",\"blockbtnc\":\"#f29900\",\"blockbtnt\":true} -->\n<section class=\"wp-block-themepark-block-themepark-area block_layout thempark-block undefined\" id=\"maxw\" style=\"\"><div class=\"block_layout_in\"><!-- wp:column {\"className\":\"block_layout_in\"} -->\n<div class=\"wp-block-column block_layout_in\"><!-- wp:themepark-block/themepark-imgtext {\"bimg\":[{\"uploading\":false,\"date\":1607588760000,\"filename\":\"about2-1.jpg\",\"menuOrder\":0,\"uploadedTo\":1925,\"type\":\"image\",\"subtype\":\"jpeg\",\"id\":3254,\"title\":\"about2\",\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy1/uploads/2020/12/about2-1.jpg\",\"link\":\"http://themetest3.local/%e9%a6%96%e9%a1%b5/about2-2\",\"alt\":\"\",\"author\":\"1\",\"description\":\"\",\"caption\":\"\",\"name\":\"about2-2\",\"status\":\"inherit\",\"modified\":1607588760000,\"mime\":\"image/jpeg\",\"icon\":\"http://themetest3.local/wp-includes/images/media/default.png\",\"dateFormatted\":\"2020年12月10日\",\"nonces\":{\"update\":\"88f50e5573\",\"delete\":\"44ee146197\",\"edit\":\"31b3cd1482\"},\"editLink\":\"http://themetest3.local/wp-admin/post.php?post=3254\\u0026action=edit\",\"meta\":false,\"authorName\":\"admin\",\"uploadedToLink\":\"http://themetest3.local/wp-admin/post.php?post=1925\\u0026action=edit\",\"uploadedToTitle\":\"首页\",\"filesizeInBytes\":84601,\"filesizeHumanReadable\":\"83 KB\",\"context\":\"\",\"height\":560,\"width\":960,\"orientation\":\"landscape\",\"sizes\":{\"thumbnail\":{\"height\":150,\"width\":150,\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy1/uploads/2020/12/about2-1-150x150.jpg\",\"orientation\":\"landscape\"},\"medium\":{\"height\":175,\"width\":300,\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy1/uploads/2020/12/about2-1-300x175.jpg\",\"orientation\":\"landscape\"},\"full\":{\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy1/uploads/2020/12/about2-1.jpg\",\"height\":560,\"width\":960,\"orientation\":\"landscape\"}},\"compat\":{\"item\":\"\",\"meta\":\"\"}}],\"bili\":50,\"bcolor\":\"#ffffff\",\"bpadding\":true,\"modle\":false,\"leftright\":true,\"bigpic\":true,\"tpvedio\":[{\"id\":2054,\"title\":\"main-slide-video2\",\"filename\":\"main-slide-video2.mp4\",\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy1/uploads/2020/10/main-slide-video2.mp4\",\"link\":\"http://themetest3.local/main-slide-video2\",\"alt\":\"\",\"author\":\"1\",\"description\":\"\",\"caption\":\"\",\"name\":\"main-slide-video2\",\"status\":\"inherit\",\"uploadedTo\":0,\"date\":\"2020-10-26T06:07:40.000Z\",\"modified\":\"2020-10-26T06:07:40.000Z\",\"menuOrder\":0,\"mime\":\"video/mp4\",\"type\":\"video\",\"subtype\":\"mp4\",\"icon\":\"http://themetest3.local/wp-includes/images/media/video.png\",\"dateFormatted\":\"2020年10月26日\",\"nonces\":{\"update\":\"2b8ab3ba56\",\"delete\":\"cd4ceda457\",\"edit\":\"4b4632470c\"},\"editLink\":\"http://themetest3.local/wp-admin/post.php?post=2054\\u0026action=edit\",\"meta\":{\"artist\":false,\"album\":false,\"bitrate\":937988,\"bitrate_mode\":false},\"authorName\":\"admin\",\"filesizeInBytes\":1247693,\"filesizeHumanReadable\":\"1 MB\",\"context\":\"\",\"width\":1280,\"height\":720,\"fileLength\":\"0:11\",\"fileLengthHumanReadable\":\"0分, 11秒\",\"image\":{\"src\":\"http://themetest3.local/wp-includes/images/media/video.png\",\"width\":48,\"height\":64},\"thumb\":{\"src\":\"http://themetest3.local/wp-includes/images/media/video.png\",\"width\":48,\"height\":64},\"compat\":{\"item\":\"\",\"meta\":\"\"}}],\"moveupd\":true} -->\n<section class=\"wp-block-themepark-block-themepark-imgtext thempark-block themepark_imgtext bigpic  padingmodle leftright moveupd undefined\" style=\"\"><div class=\"themepark_imgtext_img\" style=\"width:50%;\"><figure class=\"wp-block-video\"><video class=\"themepark_imgtext_vidio\" poster=\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy1/uploads/2020/12/about2-1.jpg\" src=\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy1/uploads/2020/10/main-slide-video2.mp4\" controls loop muted><source src=\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy1/uploads/2020/10/main-slide-video2.mp4\" type=\"video/mp4\"/></video></figure><img src=\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy1/uploads/2020/12/about2-1.jpg\" alt=\"\"/></div><div class=\"themepark_imgtext_text\" style=\"width:47%;\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:heading {\"style\":{\"typography\":{\"fontSize\":24}},\"textColor\":\"black\"} -->\n<h2 class=\"has-black-color has-text-color\" style=\"font-size:24px\">超强区块组合主题</h2>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"style\":{\"color\":{\"text\":\"#ef8800\"},\"typography\":{\"fontSize\":14}}} -->\n<p class=\"has-text-color\" style=\"color:#ef8800;font-size:14px\"><strong>基于古藤保编辑器，我们在主题首发之时开发提供了14个区块</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"style\":{\"typography\":{\"fontSize\":14},\"color\":{\"text\":\"#5d5d5d\"}}} -->\n<p class=\"has-text-color\" style=\"color:#5d5d5d;font-size:14px\">区块编辑器可以让你在古藤堡编辑器中自由的插入和组合各类区块，不同于之前WEB主题公园所发布的小工具模块排版，区块之间自由度更大，<strong>不同的区块可以插入另一个区块之中</strong>，这项功能带来了无与伦比的排版自由度.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"style\":{\"typography\":{\"fontSize\":14},\"color\":{\"text\":\"#5d5d5d\"}}} -->\n<p class=\"has-text-color\" style=\"color:#5d5d5d;font-size:14px\">在古藤堡编辑器刚推出时，因为各种不稳定因素导致了编辑器被大量用户禁用，但在经过WordPress版本迭代更新到现在，古藤堡编辑器已经非常稳定，并且迭代了更多的区块接口和函数，古藤堡编辑器也得到了全世界的用户和开发者的认可</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"style\":{\"typography\":{\"fontSize\":14},\"color\":{\"text\":\"#5d5d5d\"}}} -->\n<p class=\"has-text-color\" style=\"color:#5d5d5d;font-size:14px\">现在不仅仅是在首页，你可以在任意的页面和文章页使用区块进行排版制作你的落地页或者更丰富的产品介绍页，在WEB主题公园的主题中，<strong>你甚至可以挂载区块模板在导航、分类列表和小工具上</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:themepark-block/themepark-icon {\"txtf1\":13,\"txtc2\":\"#666666\",\"iconf\":24,\"iconl\":5,\"bc1\":\"#f5f5f5\",\"bc1o\":67,\"icon\":[{\"id\":1607567382809,\"text\":\"多模式选择\",\"text2\":\"\",\"icon\":\"fas fa-chalkboard-teacher\",\"iconc\":\"#146ba2\",\"icont\":\"\",\"url\":\"/wp-content/uploads/2020/12/about-5.png\"},{\"id\":1607567677038,\"text\":\"多区块组合\",\"text2\":\"\",\"icon\":\"fas fa-icons\",\"iconc\":\"#f29900\",\"icont\":\"\",\"url\":\"/wp-content/uploads/2020/12/about-1.png\"},{\"id\":1607567678546,\"text\":\"可重用区块\",\"text2\":\"\",\"icon\":\"fas fa-truck-loading\",\"iconc\":\"#5e8b0f\",\"icont\":\"\",\"url\":\"/wp-content/uploads/2020/12/about-2.png\"},{\"id\":1607567678938,\"text\":\"额外的模板\",\"text2\":\"\",\"icon\":\"fas fa-cogs\",\"iconc\":\"#24b6b1\",\"icont\":\"\",\"url\":\"/wp-content/uploads/2020/12/about-3.png\"},{\"id\":1607567892658,\"text\":\"后台设备预览\",\"text2\":\"\",\"icon\":\"fas fa-mobile-alt\",\"iconc\":\"#b27630\",\"icont\":\"\",\"url\":\"/wp-content/uploads/2020/12/about-4.png\"}],\"ewcss\":\"swipebox\"} -->\n<section class=\"wp-block-themepark-block-themepark-icon thempark-block themepark_icon_list_out undefined\" id=\"nomo\" style=\"padding-bottom:20px;padding-top:20px;\"><div class=\"themepark_icon_list_in\"><div class=\"themepark_icon_lists iconl5\"><a class=\"themepark_icon_box swipebox\" href=\"/wp-content/uploads/2020/12/about-5.png\"><div class=\"themepark_icon_box_i\"><div class=\"themepark_icon_box_i_out\"><i class=\"fas fa-chalkboard-teacher\" style=\"color:#146ba2;font-size:24px;\"></i></div><b style=\"font-size:13px;\">多模式选择</b></div><div class=\"themepark_icon_box_bac\" style=\"background:#f5f5f5;opacity:0.67;\"></div></a><a class=\"themepark_icon_box swipebox\" href=\"/wp-content/uploads/2020/12/about-1.png\"><div class=\"themepark_icon_box_i\"><div class=\"themepark_icon_box_i_out\"><i class=\"fas fa-icons\" style=\"color:#f29900;font-size:24px;\"></i></div><b style=\"font-size:13px;\">多区块组合</b></div><div class=\"themepark_icon_box_bac\" style=\"background:#f5f5f5;opacity:0.67;\"></div></a><a class=\"themepark_icon_box swipebox\" href=\"/wp-content/uploads/2020/12/about-2.png\"><div class=\"themepark_icon_box_i\"><div class=\"themepark_icon_box_i_out\"><i class=\"fas fa-truck-loading\" style=\"color:#5e8b0f;font-size:24px;\"></i></div><b style=\"font-size:13px;\">可重用区块</b></div><div class=\"themepark_icon_box_bac\" style=\"background:#f5f5f5;opacity:0.67;\"></div></a><a class=\"themepark_icon_box swipebox\" href=\"/wp-content/uploads/2020/12/about-3.png\"><div class=\"themepark_icon_box_i\"><div class=\"themepark_icon_box_i_out\"><i class=\"fas fa-cogs\" style=\"color:#24b6b1;font-size:24px;\"></i></div><b style=\"font-size:13px;\">额外的模板</b></div><div class=\"themepark_icon_box_bac\" style=\"background:#f5f5f5;opacity:0.67;\"></div></a><a class=\"themepark_icon_box swipebox\" href=\"/wp-content/uploads/2020/12/about-4.png\"><div class=\"themepark_icon_box_i\"><div class=\"themepark_icon_box_i_out\"><i class=\"fas fa-mobile-alt\" style=\"color:#b27630;font-size:24px;\"></i></div><b style=\"font-size:13px;\">后台设备预览</b></div><div class=\"themepark_icon_box_bac\" style=\"background:#f5f5f5;opacity:0.67;\"></div></a></div></div></section>\n<!-- /wp:themepark-block/themepark-icon --></div>\n<!-- /wp:column --></div><div class=\"themepark_imgtext_ba\" style=\"background-color:#ffffff;opacity:1;\"></div></section>\n<!-- /wp:themepark-block/themepark-imgtext --></div>\n<!-- /wp:column --></div><div class=\"layout_badh\"></div></section>\n<!-- /wp:themepark-block/themepark-area -->"
