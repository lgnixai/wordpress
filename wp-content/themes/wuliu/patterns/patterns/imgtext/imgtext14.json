  "content": "<!-- wp:themepark-block/themepark-area {\"top\":70} -->\n<section class=\"wp-block-themepark-block-themepark-area block_layout thempark-block undefined\" id=\"\" style=\"padding-top:70px;\"><div class=\"block_layout_in\"><!-- wp:column {\"className\":\"block_layout_in\"} -->\n<div class=\"wp-block-column block_layout_in\"><!-- wp:columns -->\n<div class=\"wp-block-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:themepark-block/themepark-imgtext {\"bimg\":[{\"uploading\":false,\"date\":1649732043000,\"filename\":\"jj.jpg\",\"menuOrder\":0,\"uploadedTo\":5,\"type\":\"image\",\"subtype\":\"jpeg\",\"id\":241,\"title\":\"jj\",\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/jj.jpg\",\"link\":\"http://themetest3.local/%e9%a6%96%e9%a1%b5/jj/\",\"alt\":\"\",\"author\":\"1\",\"description\":\"\",\"caption\":\"\",\"name\":\"jj\",\"status\":\"inherit\",\"modified\":1649732043000,\"mime\":\"image/jpeg\",\"icon\":\"http://themetest3.local/wp-includes/images/media/default.png\",\"dateFormatted\":\"2022年4月12日\",\"nonces\":{\"update\":\"3316d5d4e2\",\"delete\":\"8589850da8\",\"edit\":\"d3a1d89528\"},\"editLink\":\"http://themetest3.local/wp-admin/post.php?post=241\\u0026action=edit\",\"meta\":false,\"authorName\":\"admin\",\"authorLink\":\"http://themetest3.local/wp-admin/profile.php\",\"uploadedToTitle\":\"首页\",\"uploadedToLink\":\"http://themetest3.local/wp-admin/post.php?post=5\\u0026action=edit\",\"filesizeInBytes\":61674,\"filesizeHumanReadable\":\"60 KB\",\"context\":\"\",\"height\":850,\"width\":700,\"orientation\":\"portrait\",\"sizes\":{\"thumbnail\":{\"height\":150,\"width\":150,\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/jj-150x150.jpg\",\"orientation\":\"landscape\"},\"medium\":{\"height\":300,\"width\":247,\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/jj-247x300.jpg\",\"orientation\":\"portrait\"},\"full\":{\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/jj.jpg\",\"height\":850,\"width\":700,\"orientation\":\"portrait\"}},\"compat\":{\"item\":\"\",\"meta\":\"\"}}],\"bpadding\":false,\"modle\":true,\"leftright\":true,\"moveupd\":true} -->\n<section class=\"wp-block-themepark-block-themepark-imgtext thempark-block themepark_imgtext updonwmodle leftright moveupd  undefined\" style=\"\"><div class=\"themepark_imgtext_text\" style=\"\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:themepark-block/themepark-wright {\"font2\":25,\"font9\":0,\"font0\":1} -->\n<div class=\"wp-block-themepark-block-themepark-wright content-super-p\" style=\"font-size:12px;line-height:25px;padding:1px 0px;\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:heading {\"style\":{\"color\":{\"text\":\"#b0976d\"}},\"fontSize\":\"x-large\"} -->\n<h2 class=\"has-text-color has-x-large-font-size\" style=\"color:#b0976d\">Company introduction</h2>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:heading {\"style\":{\"typography\":{\"fontSize\":\"16px\"}},\"textColor\":\"white\"} -->\n<h2 class=\"has-white-color has-text-color\" style=\"font-size:16px\">公司介绍</h2>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"textColor\":\"cyan-bluish-gray\"} -->\n<p class=\"has-cyan-bluish-gray-color has-text-color\">We believe wine is something everyone can enjoy.&nbsp;</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:themepark-block/themepark-wright -->\n\n<!-- wp:themepark-block/themepark-wright {\"font2\":27,\"font9\":0} -->\n<div class=\"wp-block-themepark-block-themepark-wright content-super-p\" style=\"font-size:12px;line-height:27px;padding:10px 0px;\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"style\":{\"typography\":{\"fontSize\":\"14px\"}},\"textColor\":\"white\"} -->\n<p class=\"has-white-color has-text-color\" style=\"font-size:14px\">THEMEPARK酒业一家专业从事葡萄酒进口贸易的企业，也是国内专业的法国葡萄酒生产商、进口商之一。</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"style\":{\"typography\":{\"fontSize\":\"14px\"}},\"textColor\":\"white\"} -->\n<p class=\"has-white-color has-text-color\" style=\"font-size:14px\">旗下葡萄酒拥有法国波尔多圣埃美隆路易庄园（CHATEAU LOUIS）、香农庄园（CHATEAU JEAN BLANC）、宝兰迪庄园（CHATEAU DE BRANDEY）等欧洲酒庄的授权。</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:themepark-block/themepark-wright --></div>\n<!-- /wp:column --></div><div class=\"themepark_imgtext_img\" style=\"\"><img src=\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/jj.jpg\" alt=\"\"/></div><div class=\"themepark_imgtext_ba\" style=\"opacity:1;\"></div></section>\n<!-- /wp:themepark-block/themepark-imgtext --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:themepark-block/themepark-imgtext {\"bimg\":[{\"uploading\":false,\"date\":\"2022-04-12T03:34:03.000Z\",\"filename\":\"jj3.jpg\",\"menuOrder\":0,\"uploadedTo\":5,\"type\":\"image\",\"subtype\":\"jpeg\",\"id\":266,\"title\":\"jj3\",\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/jj3.jpg\",\"link\":\"http://themetest3.local/%e9%a6%96%e9%a1%b5/jj3/\",\"alt\":\"\",\"author\":\"1\",\"description\":\"\",\"caption\":\"\",\"name\":\"jj3\",\"status\":\"inherit\",\"modified\":\"2022-04-12T03:34:03.000Z\",\"mime\":\"image/jpeg\",\"icon\":\"http://themetest3.local/wp-includes/images/media/default.png\",\"dateFormatted\":\"2022年4月12日\",\"nonces\":{\"update\":\"32e9b22e0f\",\"delete\":\"060df85d85\",\"edit\":\"3aaa0f51f4\"},\"editLink\":\"http://themetest3.local/wp-admin/post.php?post=266\\u0026action=edit\",\"meta\":false,\"authorName\":\"admin\",\"authorLink\":\"http://themetest3.local/wp-admin/profile.php\",\"uploadedToTitle\":\"首页\",\"uploadedToLink\":\"http://themetest3.local/wp-admin/post.php?post=5\\u0026action=edit\",\"filesizeInBytes\":98027,\"filesizeHumanReadable\":\"96 KB\",\"context\":\"\",\"height\":748,\"width\":700,\"orientation\":\"portrait\",\"sizes\":{\"thumbnail\":{\"height\":150,\"width\":150,\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/jj3-150x150.jpg\",\"orientation\":\"landscape\"},\"medium\":{\"height\":300,\"width\":281,\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/jj3-281x300.jpg\",\"orientation\":\"portrait\"},\"full\":{\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/jj3.jpg\",\"height\":748,\"width\":700,\"orientation\":\"portrait\"}},\"compat\":{\"item\":\"\",\"meta\":\"\"}}],\"bpadding\":false,\"modle\":true,\"PC\":true,\"leftright\":false,\"moveupd\":true} -->\n<section class=\"wp-block-themepark-block-themepark-imgtext thempark-block themepark_imgtext updonwmodle moveupd  undefined\" style=\"\"><div class=\"themepark_imgtext_img PcOnly\" style=\"\"><img src=\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/jj3.jpg\" alt=\"\"/></div><div class=\"themepark_imgtext_text\" style=\"\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:themepark-block/themepark-wright {\"font2\":25,\"font9\":0,\"font0\":1} -->\n<div class=\"wp-block-themepark-block-themepark-wright content-super-p\" style=\"font-size:12px;line-height:25px;padding:1px 0px;\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:heading {\"style\":{\"color\":{\"text\":\"#b0976d\"}},\"fontSize\":\"x-large\"} -->\n<h2 class=\"has-text-color has-x-large-font-size\" style=\"color:#b0976d\">Brand story</h2>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:heading {\"style\":{\"typography\":{\"fontSize\":\"16px\"}},\"textColor\":\"white\"} -->\n<h2 class=\"has-white-color has-text-color\" style=\"font-size:16px\">品牌故事</h2>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"textColor\":\"cyan-bluish-gray\"} -->\n<p class=\"has-cyan-bluish-gray-color has-text-color\">We believe wine is something everyone can enjoy.&nbsp;</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:themepark-block/themepark-wright -->\n\n<!-- wp:themepark-block/themepark-wright {\"font2\":27,\"font9\":0} -->\n<div class=\"wp-block-themepark-block-themepark-wright content-super-p\" style=\"font-size:12px;line-height:27px;padding:10px 0px;\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"style\":{\"typography\":{\"fontSize\":\"14px\"}},\"textColor\":\"white\"} -->\n<p class=\"has-white-color has-text-color\" style=\"font-size:14px\">THEMEPARK多年以来在酒类行业精耕细作，与众多国际酒业巨头达成了深度战略合作，取得了红魔鬼、蒙特斯、奔富、佩兰家族、明石、长滨等知名品牌的代理权，销售渠道包括京东、天猫等各大主流电商，山姆、麦德龙、盒马等知名商超，同时拥有覆盖全国各省市区域的传统经销体系。一家专业从事葡萄酒进口贸易的企业，也是国内专业的法国葡萄酒生产商、进口商之一。</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"style\":{\"typography\":{\"fontSize\":\"14px\"}},\"textColor\":\"white\"} -->\n<p class=\"has-white-color has-text-color\" style=\"font-size:14px\"></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:themepark-block/themepark-wright --></div>\n<!-- /wp:column --></div><div class=\"themepark_imgtext_ba\" style=\"opacity:1;\"></div></section>\n<!-- /wp:themepark-block/themepark-imgtext --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns --></div>\n<!-- /wp:column --></div><div class=\"layout_badh\"></div></section>\n<!-- /wp:themepark-block/themepark-area -->"
}