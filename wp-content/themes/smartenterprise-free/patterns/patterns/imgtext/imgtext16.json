  "content": "<!-- wp:themepark-block/themepark-area {\"bcolor\":\"#1c1c1c\",\"bottom\":150,\"top\":150,\"className\":\"bom30 top30\"} -->\n<section class=\"wp-block-themepark-block-themepark-area block_layout thempark-block undefined bom30 top30\" id=\"\" style=\"background-color:#1c1c1c;padding-bottom:150px;padding-top:150px;\"><div class=\"block_layout_in\"><!-- wp:column {\"className\":\"block_layout_in\"} -->\n<div class=\"wp-block-column block_layout_in\"><!-- wp:themepark-block/themepark-imgtext {\"bimg\":[{\"uploading\":false,\"date\":1649740998000,\"filename\":\"b64dd8d19e841cf42d65912c0da4106a_red-bottle-dark.png\",\"menuOrder\":0,\"uploadedTo\":5,\"type\":\"image\",\"subtype\":\"png\",\"id\":287,\"title\":\"b64dd8d19e841cf42d65912c0da4106a_red-bottle-dark\",\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/b64dd8d19e841cf42d65912c0da4106a_red-bottle-dark.png\",\"link\":\"http://themetest3.local/%e9%a6%96%e9%a1%b5/b64dd8d19e841cf42d65912c0da4106a_red-bottle-dark/\",\"alt\":\"\",\"author\":\"1\",\"description\":\"\",\"caption\":\"\",\"name\":\"b64dd8d19e841cf42d65912c0da4106a_red-bottle-dark\",\"status\":\"inherit\",\"modified\":1649740998000,\"mime\":\"image/png\",\"icon\":\"http://themetest3.local/wp-includes/images/media/default.png\",\"dateFormatted\":\"2022年4月12日\",\"nonces\":{\"update\":\"bcdfe70c26\",\"delete\":\"76be9ff9dc\",\"edit\":\"1149beffc2\"},\"editLink\":\"http://themetest3.local/wp-admin/post.php?post=287\\u0026action=edit\",\"meta\":false,\"authorName\":\"admin\",\"authorLink\":\"http://themetest3.local/wp-admin/profile.php\",\"uploadedToTitle\":\"首页\",\"uploadedToLink\":\"http://themetest3.local/wp-admin/post.php?post=5\\u0026action=edit\",\"filesizeInBytes\":253971,\"filesizeHumanReadable\":\"248 KB\",\"context\":\"\",\"height\":253,\"width\":807,\"orientation\":\"landscape\",\"sizes\":{\"thumbnail\":{\"height\":150,\"width\":150,\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/b64dd8d19e841cf42d65912c0da4106a_red-bottle-dark-150x150.png\",\"orientation\":\"landscape\"},\"medium\":{\"height\":94,\"width\":300,\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/b64dd8d19e841cf42d65912c0da4106a_red-bottle-dark-300x94.png\",\"orientation\":\"landscape\"},\"full\":{\"url\":\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/b64dd8d19e841cf42d65912c0da4106a_red-bottle-dark.png\",\"height\":253,\"width\":807,\"orientation\":\"landscape\"}},\"compat\":{\"item\":\"\",\"meta\":\"\"}}],\"bili\":66,\"textalign2\":\"left\",\"moveupd\":true} -->\n<section class=\"wp-block-themepark-block-themepark-imgtext thempark-block themepark_imgtext moveupd  undefined\" style=\"\"><div class=\"themepark_imgtext_img\" style=\"width:66%;text-align:left;\"><img src=\"http://data.themepark.com.cn/2021/n/smartenterprise_data/qy12/uploads/2022/04/b64dd8d19e841cf42d65912c0da4106a_red-bottle-dark.png\" alt=\"\"/></div><div class=\"themepark_imgtext_text\" style=\"width:31%;\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:heading {\"textColor\":\"white\",\"fontSize\":\"x-large\"} -->\n<h2 class=\"has-white-color has-text-color has-x-large-font-size\">Chateau Louis</h2>\n<!-- /wp:heading -->\n\n<!-- wp:heading {\"textColor\":\"white\",\"fontSize\":\"medium\"} -->\n<h2 class=\"has-white-color has-text-color has-medium-font-size\">波尔多圣埃美隆路易庄园</h2>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"style\":{\"color\":{\"text\":\"#b0976d\"}},\"fontSize\":\"small\"} -->\n<p class=\"has-text-color has-small-font-size\" style=\"color:#b0976d\">葡萄酒</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"style\":{\"typography\":{\"fontSize\":\"14px\"}},\"textColor\":\"white\"} -->\n<p class=\"has-white-color has-text-color\" style=\"font-size:14px\">蒙特斯酒庄建立于1987年，是智利空加瓜谷（Colchagua Valley）著名的酒庄，智利前五大葡萄酒出口商之一。酒庄出产的佳酿在全球酒界获奖无数，有“智利国酒”之称，产品远销全球五大洲110多个国家。2010年3月20日智利总统选用蒙特斯紫天使红葡萄酒作为国宴用酒款待到访的美国总统奥巴马及其夫人。</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:buttons -->\n<div class=\"wp-block-buttons\"><!-- wp:button {\"textColor\":\"white\",\"style\":{\"border\":{\"radius\":\"5px\"},\"color\":{\"background\":\"#b0976d\"}},\"fontSize\":\"small\"} -->\n<div class=\"wp-block-button has-custom-font-size has-small-font-size\"><a class=\"wp-block-button__link has-white-color has-text-color has-background\" style=\"border-radius:5px;background-color:#b0976d\">查看品牌旗下产品</a></div>\n<!-- /wp:button --></div>\n<!-- /wp:buttons --></div>\n<!-- /wp:column --></div><div class=\"themepark_imgtext_ba\" style=\"opacity:1;\"></div></section>\n<!-- /wp:themepark-block/themepark-imgtext --></div>\n<!-- /wp:column --></div><div class=\"layout_badh\"></div></section>\n<!-- /wp:themepark-block/themepark-area -->"
